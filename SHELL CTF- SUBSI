text = 'HESL{LRAT5PN51010T_CNPH1R}3'
key = [3,1,2]
li0 = []
li1 = []
li2 = []
flag=''
for i in range(0,len(text)):
	if((i-2)%3==0):
		li0.append(text[i])
	elif ((i-1)%3==0):
		li1.append(text[i])
	elif((i)%3==0):
		li2.append(text[i])
for i in range(len(li0)):
	flag+=li0[i]
	flag+=li2[i]
	flag+=li1[i]
print(flag)
#simple transposition cipher
